{
    "name": "Youtube Playlist Length Calculator ",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
        "128": "play.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/playlist?*"
            ],
            "js": [
                "app.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ]
}